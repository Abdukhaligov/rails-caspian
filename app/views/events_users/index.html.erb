<p id="notice"><%= notice %></p>

<h1>Events Users</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Event</th>
      <th>Membership</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @events_users.each do |events_user| %>
      <tr>
        <td><%= events_user.user %></td>
        <td><%= events_user.event %></td>
        <td><%= events_user.membership %></td>
        <td><%= events_user.status %></td>
        <td><%= link_to 'Show', events_user %></td>
        <td><%= link_to 'Edit', edit_events_user_path(events_user) %></td>
        <td><%= link_to 'Destroy', events_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Events User', new_events_user_path %>
