<p id="notice"><%= notice %></p>

<h1>User Memberships</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_memberships.each do |user_membership| %>
      <tr>
        <td><%= link_to 'Show', user_membership %></td>
        <td><%= link_to 'Edit', edit_user_membership_path(user_membership) %></td>
        <td><%= link_to 'Destroy', user_membership, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User Membership', new_user_membership_path %>
